<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="com.example.test.mapper.RecruitMapper">
 	<select id="selectRecruitList" resultType="com.example.test.domain.Recruit">
 		SELECT
 			`id`
			,`companyId`
			,`position`
			,`responsibilities`
			,`qualification`
			,`type`
			,`region`
			,`salary`
			,`deadline`
			,`createdAt`
			,`updatedAt`
 		FROM
 			`recruit`
 	</select>
 </mapper>
